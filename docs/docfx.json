{
  "build": {
    "content": [
      {
        "src": ".",
        "dest": ".",
        "files": [
          "**/*.md",
          "**/toc.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/*.liquid.md",
          "**/_css/**",
          "**/_includes/**",
          "**/_bread/**",
          "**/_themes/**"
        ]
      },
      {
        "src": "_bread",
        "dest": "bread",
        "files": [
          "**/*.yml"
        ]
      }
    ],
    "resource": [
      {
        "src": ".",
        "dest": ".",
        "files": [
          "**/*.css",
          "**/*.png",
          "**/*.jpg",
          "**/*.jpeg",
          "**/*.gif",
          "**/*.svg",
          "feed.xml"
        ],
        "exclude": [
          "**/obj/**",
          "**/_themes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "feedback_system": "GitHub",
      "feedback_github_repo": "mspnp/architecture-center",
      "breadcrumb_path": "/azure/architecture/bread/toc.json",
      "brand": "azure",
      "ms.author": "pnp",
      "ms.service": "guidance",
      "ms.topic": "article",
      "searchScope": [
        "Azure"
      ]
    },
    "fileMetadata": {
      "tocRel": {
        "reference-architectures/**.md": "../toc.json"
      },
      "product_feedback_displaytext": {
        "example-scenario/**/*.md": "Scenario Feedback",
        "cloud-adoption/**/*.md": "Cloud Adoption Feedback"
      },
      "feedback_product_url": {
        "example-scenario/**/*.md": "https://azure-architecture.uservoice.com/forums/918625-architecture-guidance",
        "cloud-adoption/**/*.md": "https://azure-architecture.uservoice.com/forums/918757-cloud-adoption"
      }
    },
    "template": [
      "docs.html"
    ],
    "dest": "azure",
    "markdownEngineName": "markdig"
  }
}
